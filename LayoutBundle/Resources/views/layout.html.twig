<html>
    <head>
        <title>Surveillance {{title|default('')}}</title>
        {{css()}}
        {{js()}}
    </head>
    <body>

{% set routes = [ {"name":"hypo_test_default_index", "title":"index"}, {"name":"hypo_test_file", "title":"file"}, {"name":"hypo_test_image", "title":"image"} ] %}
{% if routes is defined %}
    <nav>
        {% for route in routes %}
            {% set url = url(route.name), title=route.title|capitalize %}
            <a href="{{url}}" title="{{title|trans|raw}}">
                {{title|trans|raw}}
            </a>
        {% endfor %}       
    </nav>
{% endif %}
    {{content|raw}}
    </body>
</html>

